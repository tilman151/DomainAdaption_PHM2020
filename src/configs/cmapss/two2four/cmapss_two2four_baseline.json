{
    "name": "cmapss_two2four_baseline",

    "dataset": {
        "name": "datasets.domain_adaption.no_adaption_dataset",
        "parameters": {
            "source_dataset": {
                "name": "datasets.cmapss.cmapss",
                "parameters": {
                    "fd": 2,
                    "window_size": 30
                }
            },
            "target_dataset": {
                "name": "datasets.cmapss.cmapss",
                "parameters": {
                    "fd": 4,
                    "window_size": 30
                }
            },
            "batch_size": 512,
            "shuffle": true
        }
    },

    "model": {
        "name": "models.rul.BaselineNetwork",
        "parameters": {
            "in_channels": 14,
            "seq_len": 30,
            "base_filters": 10,
            "num_layers": 5,
            "num_common_layers": 5,
            "kernel_size": 10,
            "num_classes": 1,
            "dim_fc": 100,
            "dropout_rate": 0.5
        }
    },

    "trainer": {
        "name": "trainers.regression.RMSETrainer",
        "parameters": {
            "optim": {
                "name": "torch.optim.Adam",
                "parameters": {
                    "lr": 0.001
                }
            }
        }
    },

    "metrics": [
        "metrics.build_in.RMSE",
        "metrics.build_in.RULScore"
    ]
}