{
    "name": "cmapss_one2two_rtn",

    "dataset": {
        "name": "datasets.domain_adaption.deep_adaption_dataset",
        "parameters": {
            "source_dataset": {
                "name": "datasets.cmapss.cmapss",
                "parameters": {
                    "fd": 1,
                    "window_size": 30
                }
            },
            "target_dataset": {
                "name": "datasets.cmapss.cmapss",
                "parameters": {
                    "fd": 2,
                    "window_size": 30
                }
            },
            "batch_size": 512,
            "shuffle": true
        }
    },

    "model": {
        "name": "models.rul.RTNNetwork",
        "parameters": {
            "in_channels": 14,
            "seq_len": 30,
            "base_filters": 10,
            "num_layers": 5,
            "num_common_layers": 5,
            "kernel_size": 10,
            "num_classes": 1,
            "dim_fc": 100,
            "dropout_rate": 0.5
        }
    },

    "trainer": {
        "name": "trainers.regression.RTNTrainer",
        "parameters": {
            "optim": {
                "name": "torch.optim.Adam",
                "parameters": {
                    "lr": 0.001
                }
            },
            "mmd_lmbd": 10.0,
            "entropy_lmbd": 0.0,
            "scale_lr": false
        }
    },

    "metrics": [
        "metrics.domain_adaption.RMSE",
        "metrics.domain_adaption.RULScore"
    ]
}